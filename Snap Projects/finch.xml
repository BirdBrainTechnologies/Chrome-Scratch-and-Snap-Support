) {&#xD;    var getValue = function(response) {&#xD;        sensorvalue = response.temperature;&#xD;        callback(sensorvalue);&#xD;    };&#xD;    console.log("sending message to chrome");&#xD;    chrome.runtime.sendMessage(finchAppID, {&#xD;        message: "POLL"&#xD;    }, getValue);&#xD;    console.log("sent message to chrome");&#xD;};&#xD;&#xD;function processValue(sensorvalue) {&#xD;    &#xD;    console.log("sensor value has been set: ", sensorvalue);&#xD;    myself.finchDummy9 = sensorvalue;&#xD;};&#xD;getSensorData(processValue);</l></block><list></list></block><block s="doWait"><l>0.01</l></block><block s="doReport"><block s="evaluate"><block s="reportJSFunction"><list></list><l>return this.finchDummy9;</l></block><list></list></block></block></script></block-definition><block-definition s="Finch Temperature Fahrenheit" type="reporter" category="sensing"><header></header><code></code><inputs></inputs><script><block s="doReport"><block s="reportSum"><block s="reportProduct"><l>1.8</l><custom-block s="Finch Temperature Celcius"></custom-block></block><l>32</l></block></block></script></block-definition></blocks><variables></variables></project>